{
  "name": "@cordova-plugin/aws-base",
  "version": "0.6.0",
  "description": "Cordova Plugin for AWS",
  "main": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "postinstall": "tsc && _inject_pre AWS_REGION AWS_COGNITO_POOL_ID || echo 'No scripts'"
  },
  "bin": {
    "_inject_pre": "./scripts/_inject_pre.js",
    "add_plugin": "./scripts/add_plugin.js",
    "web_inject": "./scripts/web_inject.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fathens/Cordova-Plugin-AWS_Base.git"
  },
  "author": "f:athens",
  "license": "MIT",
  "dependencies": {
    "himalaya": "^0.2.2",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "typescript": "~2.0.10"
  },
  "peerDependencies": {
    "typescript": "~2.0.10"
  }
}
